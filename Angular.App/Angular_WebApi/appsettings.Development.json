    //"ConnectionString": "Server =172.20.1.20\\DEV; Database=AngularDB;User Id = sa;Password=soft157703ware; MultipleActiveResultSets=true; Encrypt = false" //  Server
  },



  "AutoMapper": {
    "AssemblyNamesForLoadProfiles": "Angular_WebApi"
  },





  "RabbitMq": {
    "PerssistMessage": false,
    "ExchangeName": "DesktopExchange",
    "ServiceName": "Desktop",
    "Url": "amqp://guest:guest@localhost:5672/"
