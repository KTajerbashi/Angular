  "Swagger": {
    "Enabled": true,
    "SwaggerDoc": {
      "Enabled": true,
      "Version": "v1",
      "Description": "Please enter token",
      "BearerFormat": "JWT",
      "Scheme": "bearer",
      "Id": "Bearer",
      "Title": "DesktopMashhad",
      "Name": "v1",
      "URL": "/swagger/v1/swagger.json"
    },
    "OAuth": {
      "Enabled": false,
      "AuthorizationUrl": "https://github.com/Server =TAJERBASHI/Desktop_Solution",
      "TokenUrl": "https://github.com/KServer =TAJERBASHI/CleanArchitecture",
      "Scopes": {
        "DesktopScope": "DesktopScope"
      }
    }

  },



  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "TK": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "..\\Logs\\Desktop.Log.txt" }
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "ConnectionString": "Server =TAJERBASHI; Database=AngularDB;User Id = sa;Password=1qaz!QAZ; MultipleActiveResultSets=true; Encrypt = false", //  Server
          "sinkOptionsSection": {
            "tableName": "EventLogs",
            "schemaName": "Log",
            "autoCreateSqlTable": true
          },
          "columnOptionsSection": {
            "additionalColumns": [
              {
                "ColumnName": "UserId",
                "DataType": "bigint"
              },
              {
                "ColumnName": "UserName",
                "DataType": "nvarchar",
                "DataLength": 100
              },
              {
                "ColumnName": "LogEventId",
                "DataType": "nvarchar",
                "DataLength": 100
              },
              {
                "ColumnName": "TraceId",
                "DataType": "nvarchar"
              },
              {
                "ColumnName": "Category",
                "DataType": "nvarchar",
                "DataLength": 100
              },
              {
                "ColumnName": "IP",
                "DataType": "nvarchar",
                "DataLength": 30
              },
              {
                "ColumnName": "UserAgent",
                "DataType": "nvarchar"
              },
              {
                "ColumnName": "CallSite",
                "DataType": "nvarchar",
                "DataLength": 50
              },
              {
                "ColumnName": "Parameter",
                "DataType": "nvarchar"
              },
              {
                "ColumnName": "Property",
                "DataType": "nvarchar"
              },
              {
                "ColumnName": "RecordHash",
                "DataType": "nvarchar"
              }
            ]
          }
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:7289",
          "indexFormat": "DesktopMashhad-serilog-sample-index-{0:yyyy.MM}",
          "restrictedToMinimumLevel": "Verbose",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv6",
          "batchPostingLimit": 50,
          "period": 2,
          "inlineFields": true,
          "bufferFileSizeLimitBytes": 5242880,
          "bufferLogShippingInterval": 5000,
          "bufferRetainedInvalidPayloadsLimitBytes": 5000,
          "bufferFileCountLimit": 31,
          "connectionTimeout": 5,
          "emitEventFailure": "WriteToSelfLog",
          "queueSizeLimit": "100000"
        }
      }
    ]
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "30s",
        "Limit": 3
      }
    ],
    "ClientRules": []
  },
  "IpRateLimitPolicies": {
    "EndpointRateLimitPolicy": {
      "ClientIdHeader": "X-ClientId",
      "Period": "30s",
      "Limit": 3,
      "Rules": [
        {
          "Endpoint": "*",
          "Period": "30s",
          "Limit": 3
        }
      ]
    }
  },
  "AllowedHosts": "*"
}